{
    "name": "@banquette/monorepo",
    "version": "0.0.0",
    "description": "Write less, cleaner code and focus on building amazing apps!",
    "author": "Julien Pinto",
    "license": "Apache-2.0",
    "scripts": {
        "app/vite": "pnpm --filter @app/vite",
        "app/nuxt": "pnpm --filter @app/nuxt",
        "api": "pnpm --filter @banquette/api",
        "_config": "pnpm --filter @banquette/config",
        "dependency-injection": "pnpm --filter @banquette/dependency-injection",
        "dom-modules": "pnpm --filter @banquette/dom-modules",
        "event": "pnpm --filter @banquette/event",
        "exception": "pnpm --filter @banquette/exception",
        "fingerprint": "pnpm --filter @banquette/fingerprint",
        "form": "pnpm --filter @banquette/form",
        "http": "pnpm --filter @banquette/http",
        "inversify": "pnpm --filter @banquette/inversify",
        "log": "pnpm --filter @banquette/log",
        "model": "pnpm --filter @banquette/model",
        "model-form": "pnpm --filter @banquette/model-form",
        "model-validation": "pnpm --filter @banquette/model-validation",
        "object-observer": "pnpm --filter @banquette/object-observer",
        "promise": "pnpm --filter @banquette/promise",
        "storage": "pnpm --filter @banquette/storage",
        "ui": "pnpm --filter @banquette/ui",
        "utils-array": "pnpm --filter @banquette/utils-array",
        "utils-crypto": "pnpm --filter @banquette/utils-crypto",
        "utils-date": "pnpm --filter @banquette/utils-date",
        "utils-dom": "pnpm --filter @banquette/utils-dom",
        "utils-easing": "pnpm --filter @banquette/utils-easing",
        "utils-glob": "pnpm --filter @banquette/utils-glob",
        "utils-misc": "pnpm --filter @banquette/utils-misc",
        "utils-object": "pnpm --filter @banquette/utils-object",
        "utils-random": "pnpm --filter @banquette/utils-random",
        "utils-reflection": "pnpm --filter @banquette/utils-reflection",
        "utils-string": "pnpm --filter @banquette/utils-string",
        "utils-type": "pnpm --filter @banquette/utils-type",
        "validation": "pnpm --filter @banquette/validation",
        "vue-dom-module": "pnpm --filter @banquette/vue-dom-module",
        "vue-material-icons": "pnpm --filter @banquette/vue-material-icons",
        "vue-remix-icons": "pnpm --filter @banquette/vue-remix-icons",
        "vue-typescript": "pnpm --filter @banquette/vue-typescript",
        "vue-ui": "pnpm --filter @banquette/vue-ui",
        "dev": "pnpm --recursive --parallel --stream run dev",
        "build": "node utils/build-all.mjs",
        "test": "jest",
        "generate-icons": "node --max-old-space-size=32768 utils/generate-icons-components.mjs",
        "show-dependencies": "node utils/show-packages-dependencies.mjs",
        "clear-dists": "node utils/clear-builds.mjs",
        "clear-node-modules": "node utils/clear-node-modules.mjs",
        "build:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.1",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/jest": "^29.5.12",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "@vue/vue3-jest": "^29.2.6",
        "acorn": "^8.11.3",
        "chalk": "^5.3.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-vue": "^9.21.1",
        "fs-extra": "^11.2.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-extended": "^3.2.4",
        "jsdom": "^20.0.3",
        "prettier": "^2.8.8",
        "rollup-plugin-visualizer": "^5.12.0",
        "svg-parser": "^2.0.4",
        "svg-path-bbox": "^1.2.4",
        "svgo": "^3.2.0",
        "syncpack": "^9.8.6",
        "terser": "^5.27.2",
        "ts-essentials": "^9.4.1",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3",
        "vite-plugin-tree-shakable-decorators": "^0.1.2"
    }
}
