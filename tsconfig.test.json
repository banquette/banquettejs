{
    "compilerOptions": {
        "target": "es5",
        "lib": ["es6", "es5", "dom"],
        "module": "es2015",
        "strict": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "paths": {
            "@banquette/api": ["packages/api/src/index.ts"],
            "@banquette/config": ["packages/config/src/index.ts"],
            "@banquette/dependency-injection": [
                "packages/dependency-injection/src/index.ts"
            ],
            "@banquette/dom-modules": ["packages/dom-modules/src/index.ts"],
            "@banquette/event": ["packages/event/src/index.ts"],
            "@banquette/exception": ["packages/exception/src/index.ts"],
            "@banquette/fingerprint": ["packages/fingerprint/src/index.ts"],
            "@banquette/form": ["packages/form/src/index.ts"],
            "@banquette/http": ["packages/http/src/index.ts"],
            "@banquette/inversify": ["packages/inversify/src/index.ts"],
            "@banquette/log": ["packages/log/src/index.ts"],
            "@banquette/model": ["packages/model/src/index.ts"],
            "@banquette/model-api": ["packages/model-api/src/index.ts"],
            "@banquette/model-form": ["packages/model-form/src/index.ts"],
            "@banquette/model-validation": [
                "packages/model-validation/src/index.ts"
            ],
            "@banquette/promise": ["packages/promise/src/index.ts"],
            "@banquette/storage": ["packages/storage/src/index.ts"],
            "@banquette/utils-array/*": ["packages/utils-array/src/*"],
            "@banquette/utils-crypto/*": ["packages/utils-crypto/src/*"],
            "@banquette/utils-date/*": ["packages/utils-date/src/*"],
            "@banquette/utils-dom/*": ["packages/utils-dom/src/*"],
            "@banquette/utils-easing/*": ["packages/utils-easing/src/*"],
            "@banquette/utils-glob": ["packages/utils-glob/src/index.ts"],
            "@banquette/utils-misc/*": ["packages/utils-misc/src/*"],
            "@banquette/utils-object/*": ["packages/utils-object/src/*"],
            "@banquette/utils-random/*": ["packages/utils-random/src/*"],
            "@banquette/utils-reflection/*": [
                "packages/utils-reflection/src/*"
            ],
            "@banquette/utils-string/*": ["packages/utils-string/src/*"],
            "@banquette/utils-type/*": ["packages/utils-type/src/*"],
            "@banquette/validation": ["packages/validation/src/index.ts"],
            "@banquette/vue-dom-module": [
                "packages/vue-dom-module/src/index.ts"
            ],
            "@banquette/vue-material-icons": [
                "packages/vue-material-icons/src/index.ts"
            ],
            "@banquette/vue-typescript": [
                "packages/vue-typescript/src/index.ts"
            ]
        },
        "baseUrl": ".",
        "typeRoots": ["node_modules/@types", "./packages/vue-core/src/@types"]
    }
}
